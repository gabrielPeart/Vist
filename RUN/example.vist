

type Foo {
    let a: Int = 10
    var b: Int = 20
    let c: Int = 40
    
    func sum: Int -> Int = do
        return (self.a + self.b + self.c) * $0
        
    func sum: Bool = do
        if $0 do print(self.a)
}


let f = Foo()
f.sum(true)
let u = f.sum(2)

print(u)



