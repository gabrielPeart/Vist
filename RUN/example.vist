


// type foo, { i64 i64 } type
// implicit memberwise initialser is added
type StackOf2 {
    var a: Int
    var b: Int
    
    func sum :: () -> Int = do
        return a + b
    
    @mutating
    func pop :: () -> Int = {
        let v = b
        b = 0
        return v
    }
}
var stack = StackOf2 2 10

print (stack.pop ())

print stack.b

print (stack.sum ())

