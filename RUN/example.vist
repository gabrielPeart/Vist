


concept Eq {
    var a: Int, b: Int
    
    func sum :: -> Int = do return 1
}


type Bar {
    var x: Bool, b: Int, a: Int
    
    func sum :: -> Int = do
        return a + b
}


func foo :: Eq Int -> Int = (a b) do
    return a.a + b

let bar = Bar true 11 4
let foo = foo bar 2

print foo
