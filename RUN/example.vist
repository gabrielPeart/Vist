

concept Eq {
    var a: Int, b: Int
    
    func sum :: -> Int
}


type Bar {
    var x: Bool, b: Int, a: Int
    
    func sum :: -> Int = do
    return a + b
}


func foo :: Eq Int -> Int = (a b) do
    return a.sum () + b
func foo2 :: Eq Int -> Int = (a b) do
    return a.a + (a.b * 2) + b

let bar = Bar true 11 4
print (foo bar 2) // test: 17
print (foo2 bar 2) // test: 28

