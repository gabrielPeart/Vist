

type Foo {
    let a: Int = 10
    var b: Int = 20
    let c: Int = 40
    
    func sum: Int -> Int = do
        return (self.a + self.b + self.c) * $0
}


let f = Foo()
let u = f.sum(2)

print(u)


type Bar {
    let aay: Bool
    let i: Int
    
    init Int Bool = {
        i = $0
        aay = $1
    }
    
    func meme: () = do
        if self.aay do print(self.i) else do print(0)
}

let w = Bar(100 true)
w.meme()
