

type Int {
    var value: LLVM.Int64

    init Int = |o| {
        value = o.value
    }

    init LLVM.Int = |v| {
        value = v
    }

}

func foo: Int Int -> Int = do
    return Int($0.value + $1.value)

let a = Int(3)
print(a.value)

let b = Int(a)
